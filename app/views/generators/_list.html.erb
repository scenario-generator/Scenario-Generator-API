<!-- Table of generators -->

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Active?</th>
      <th>Actions</th>
    </tr>
  </thead>
  <tbody>
    <% generators.each do |generator| %>
      <tr>
        <td><%= generator.name %></td>
        <td><%= generator.active ? "Yes" : "No" %></td>
        <td>
          <%= link_to 'Show', generator %>
          <%= link_to 'Edit', edit_generator_path(generator) %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>
